<?xml version="1.0"?>
<Node 	 name="Scene"  gravity="0 0 0"  dt="1"  >
    <RequiredPlugin name="SofaOpenglVisual"/>
	<RequiredPlugin name="Registration"  printLog="1"  pluginName="Registration" />
	<VisualStyle name="visualStyle1"  displayFlags="showVisual showForceFields" />
	<EulerImplicitSolver name="eulerImplicitSolver1"  rayleighStiffness="0.1"  rayleighMass="0.1" />
	<CGLinearSolver template="GraphScattered" name="cGLinearSolver1"  iterations="15"  threshold="1e-08" tolerance="1e-5"/>
	<Node 	 name="target"  >
                <MeshOBJLoader name="meshLoader_0" filename="data/femur_m.obj"  translation="3 2 1"  rotation="30 40 50"  scale3d="1 1 1" handleSeams="1" />
                <OglModel template="ExtVec3f" name="Femur1" src="@meshLoader_0"  material="Default Diffuse 1 0.494118 0.4 1 1 Ambient 1 0.0941176 0.0784314 0.2 1 Specular 0 0.494118 0.4 1 1 Emissive 0 0.494118 0.4 1 1 Shininess 0 45 "  primitiveType="DEFAULT"  blendEquation="GL_FUNC_ADD"  sfactor="GL_SRC_ALPHA"  dfactor="GL_ONE_MINUS_SRC_ALPHA" />
                <MeshOBJLoader name="meshLoader"  filename="data/femur_m.obj"  triangulate="1"  translation="3 2 1"  rotation="30 40 50"  scale3d="1 1 1" />
		<GenerateRigidMass template="Rigid3d" name="Rigid"  position="@meshLoader.position"  triangles="@meshLoader.triangles"  quads="@meshLoader.quads"  polygons="@meshLoader.polygons" />
	</Node>
	<Node 	 name="source"  >
                <MeshOBJLoader name="meshLoader_1" filename="data/femur_m.obj"  translation="0 0 0"  rotation="10 40 30"  scale3d="0.8 0.5 0.7" handleSeams="1" />
                <OglModel template="ExtVec3f" name="Femur2" src="@meshLoader_1"  material="Default Diffuse 1 1 0.4 0.494118 1 Ambient 1 0 0 0 1 Specular 0 1 0.4 0.494118 1 Emissive 0 1 0.4 0.494118 1 Shininess 0 45 "  primitiveType="DEFAULT"  blendEquation="GL_FUNC_ADD"  sfactor="GL_SRC_ALPHA"  dfactor="GL_ONE_MINUS_SRC_ALPHA" />
                <MeshOBJLoader name="meshLoader"  filename="data/femur_m.obj"  triangulate="1"  translation="@Femur2.translation"  rotation="@Femur2.rotation"  scale3d="0.8 0.5 0.7" />
		<GenerateRigidMass template="Rigid3d" name="Rigid"  position="@meshLoader.position"  triangles="@meshLoader.triangles"  quads="@meshLoader.quads"  polygons="@meshLoader.polygons" />
	</Node>
	<Node 	 name="InertiaAlign"  >
                <InertiaAlign name="Align"  targetCenter="@../target/Rigid.centerToOrigin"  sourceCenter="@../source/Rigid.centerToOrigin"  targetInertiaMatrix="@../target/Rigid.inertiaMatrix"  sourceInertiaMatrix="@../source/Rigid.inertiaMatrix"  sourcePosition="@../source/meshLoader.position"  targetPosition="@../target/meshLoader.position" />
                <MechanicalObject template="Vec3d" name="Placed"  position="@Align.sourcePosition"  velocity="0 0 0"  force="0 0 0"  externalForce="0 0 0"  derivX="0 0 0"  restScale="1"  showObject="1"  showObjectScale="1"  isToPrint="1" />
	</Node>
</Node>
