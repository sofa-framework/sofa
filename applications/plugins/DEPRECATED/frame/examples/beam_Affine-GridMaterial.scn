		<Node name="Visu">
			<OglModel name="Beam" fileMesh="mesh/beam.obj" normals="0"   color="1 .8 .3 1" />
			<!--
			<FrameBlendingMapping template="FrameAffine,ExtVec3f" name="SMapping"  input="@../DOFs"  output="@Beam"/>
			-->
		</Node>


	</Node>
</Node>
