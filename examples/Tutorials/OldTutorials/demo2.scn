            </Node>
        </Node>
        <Node name="M3">
            <Object type="MechanicalObject" filename="BehaviorModels/smCube27.xs3" dz="21" />
            <Object type="DiagonalMass" filename="BehaviorModels/smCube27.xs3" />
            <Object type="StiffSpringForceField" name="Springs" filename="BehaviorModels/smCube27.xs3" />
        </Node>
        <Node name="M4">
            <Object type="MechanicalObject" template="Rigid" dz="31.5" />
            <Object type="UniformMass" filename="BehaviorModels/rigid.xs3" />
            <Node name="Points">
                <Object type="MechanicalObject" template="Vec3d" filename="BehaviorModels/smCube27.xs3" />
                <Object type="RigidMapping" />
            </Node>
        </Node>
        <Node name="M5">
            <Object type="MechanicalObject" filename="BehaviorModels/smCube27.xs3" dz="42" />
