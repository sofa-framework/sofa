<Node name="TorusSpring">
	<Object type="EulerImplicitSolver" rayleighStiffness="0.01" />
	<Object type="CGLinearSolver" iterations="25" />
	<Object type="MeshGmshLoader" name="meshLoader" filename="mesh/torus_low_res.msh"/>
	<Object type="Mesh" src="@meshLoader"/>
	<Object type="MechanicalObject"  dx="0" dy="0" dz="0" rx="0" ry="0" rz="0" scale="1.0"/>
	<Object type="UniformMass" totalmass="5" />
	<Object type="MeshSpringForceField" name="Springs" tetrasStiffness="400" tetrasDamping="4"/>
	<Node name="Visu">
		<Object type="OglModel" name="Visual" fileMesh="mesh/torus.obj"  color="green"  dx="0" dy="0" dz="0" rx="0" ry="0" rz="0" scale="1.0"/>
		<Object type="BarycentricMapping" object1="../.." object2="Visual" />
	</Node>
	<Node name="Surf2">
   <Object type="MeshObjLoader" name="meshLoader" filename="mesh/torus_for_collision.obj"/>
		<Object type="Mesh" src="@meshLoader"/>
		<Object type="MechanicalObject"  dx="0" dy="0" dz="0" rx="0" ry="0" rz="0" scale="1.0" />
		<Object type="Triangle" />
		<Object type="Line" />
		<Object type="Point" />
		<Object type="BarycentricMapping" />
	</Node>
</Node>
