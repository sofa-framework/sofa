<Node name="MeshSpring">
	<Object type="EulerImplicitSolver" rayleighStiffness="0.01" />
	<Object type="CGLinearSolver" iterations="25" threshold="0.00000001" />	 
	<Object type="MeshLoader" name="mechaLoader" filename="file"/>
	<Object type="Mesh"/> 
	<Object type="MechanicalObject" translation="0 0 0" rotation="0 0 0" scale3d="1.0 1.0 1.0"/>
	<Object type="UniformMass" totalmass="5" />
	<Object type="MeshSpringForceField" name="Springs" stiffness="400" damping="4"/>
	<Node name="VisualNode">
		<Object type="OglModel" name="Visual" filename="file"  color="green" dx="0" dy="0" dz="0" rx="0" ry="0" rz="0" scale="1.0" />
		<Object type="BarycentricMapping" object1="../.." object2="Visual" />
	</Node>
	<Node name="CollisionNode">
	        <Object type="MeshLoader" name="collisionLoader"  filename="file"/>
		<Object type="Mesh" />
		<Object type="MechanicalObject" useMask="true"  translation="0 0 0" rotation="0 0 0" scale3d="1.0 1.0 1.0"/>
		<Object type="Triangle" />
		<Object type="Line" />
		<Object type="Point" />
		<Object type="BarycentricMapping" />
	</Node>
</Node>
