<Node name="root" dt="0.02" gravity="0 -10 0">

    <include href="../FEMBAR-common.xml"/>

    <ShewchukPCGLinearSolver name="PCG" iterations="1000" preconditioners="preconditioner" update_step="1"/>
    <WarpPreconditioner name="preconditioner" solverName="factorization" update_step="15"/>
    <AsyncSparseLDLSolver name="factorization" template="CompressedRowSparseMatrixMat3x3d"/>
    <HexahedronFEMForceField name="FEM" youngModulus="4000" poissonRatio="0.3" method="large" />

</Node>
